<view class="cml-checkbox  cml-base cml-view cml-fca93ac6">
  <view class="cml-checkbox-wrap  cml-base cml-view cml-fca93ac6" onTap="_cmlEventProxy" data-eventtap="changeCheck">
    <view class="cml-checkbox-check  cml-base cml-view cml-fca93ac6" style="{{checkStyle}}">
      <view class="{{ checkClass }}  cml-base cml-view cml-fca93ac6">
        <image class="inner-icon  cml-base cml-image cml-fca93ac6" src="{{ checkImg }}" />
      </view>
    </view>
    <text class="cml-checkbox-label  cml-base cml-text cml-fca93ac6">{{label}}</text>
  </view>
</view>