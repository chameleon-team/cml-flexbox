<view class="cml-checkbox  cml-base cml-view cml-16265805">
  <view class="cml-checkbox-wrap  cml-base cml-view cml-16265805" onTap="_cmlEventProxy" data-eventtap="{{['changeCheck']}}">
    <view class="cml-checkbox-check  cml-base cml-view cml-16265805" style="{{checkStyle}}">
      <view class="{{ checkClass }}  cml-base cml-view cml-16265805">
        <image class="inner-icon  cml-base cml-image cml-16265805" src="{{ checkImg }}" />
      </view>
    </view>
    <text class="cml-checkbox-label  cml-base cml-text cml-16265805">{{label}}</text>
  </view>
</view>